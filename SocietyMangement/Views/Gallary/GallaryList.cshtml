@{
    Layout = "_Layout_Frontend";
}

<div class="hero">
    <div class="hero-slide">
        <div class="img overlay"
             style="background-image: url('../Frontend/images/Area.jpg')"></div>
        <div class="img overlay"
             style="background-image: url('../Frontend/images/Area1.jpg')"></div>
        <div class="img overlay"
             style="background-image: url('../Frontend/images/Area3.jpg')"></div>
    </div>

    <div class="container">
        <div class="row justify-content-center align-items-center">
            <div class="col-lg-9 text-center">
                <h1 class="heading" data-aos="fade-up">
                    Welcome To MySociety
                </h1>
                <form action="#"
                      class="narrow-w form-search d-flex align-items-stretch mb-3"
                      data-aos="fade-up"
                      data-aos-delay="200">
                    <input type="text"
                           class="form-control px-4"
                           placeholder="Your ZIP code or City. e.g. New York" />
                    <button type="submit" class="btn btn-primary">Search</button>
                </form>
            </div>
        </div>
    </div>
</div>
<div class="section">
    <div class="container">
        <div class="row mb-5 align-items-center">
            <div class="col-lg-6">
                <h2 class="font-weight-bold text-primary heading">
                   Gallary
                </h2>
            </div>
            <div class="col-lg-6 text-lg-end">
                <p>
                    <a href="#"
                       target="_blank"
                       class="btn btn-primary text-white py-3 px-4">View all Gallary</a>
                </p>
            </div>
        </div>
        <div class="row">
            <div class="col-12">
                <div class="property-slider-wrap">
                    <div class="property-slider">

                        <!-- Gym Card -->
                        <div class="property-item">
                            <a href="property-single.html" class="img">
                                <img src="~/Frontend/images/Gym3.jpg" alt="Gym" class="img-fluid" style="width: 100%; height: 500px;" />
                            </a>
                            <div class="property-content">
                                <div>
                                    <span class="city d-block mb-3">Gym</span>
                                    <span class="d-block mb-2 text-black-50">A state-of-the-art fitness center equipped with modern machines and free weights. The gym also includes personal training options, yoga sessions, and a relaxation area.</span>
                                    <a href="property-single.html" class="btn btn-primary py-2 px-3">See details</a>
                                </div>
                            </div>
                        </div>

                        <!-- Library Card -->
                        <div class="property-item">
                            <a href="property-single.html" class="img">
                                <img src="~/Frontend/images/Liabarary1.jpg" alt="Library" class="img-fluid" style="width: 100%; height: 500px;" />
                            </a>
                            <div class="property-content">
                                <div>
                                    <span class="city d-block mb-3">Library</span>
                                    <span class="d-block mb-2 text-black-50">An expansive library featuring thousands of books, journals, and digital resources. The library includes study cubicles, a discussion room, and a dedicated area for e-learning.</span>
                                    <a href="property-single.html" class="btn btn-primary py-2 px-3">See details</a>
                                </div>
                            </div>
                        </div>

                        <!-- Children's Play Area Card -->
                        <div class="property-item">
                            <a href="property-single.html" class="img">
                                <img src="~/Frontend/images/Play.jpg" alt="Children's Play Area" class="img-fluid" style="width: 100%; height: 500px;" />
                            </a>
                            <div class="property-content">
                                <div>
                                    <span class="city d-block mb-3">Children's Play Area</span>
                                    <span class="d-block mb-2 text-black-50">A vibrant play area filled with slides, swings, climbing structures, and sandbox play zones. Safety mats are installed to ensure children’s security during playtime.</span>
                                    <a href="property-single.html" class="btn btn-primary py-2 px-3">See details</a>
                                </div>
                            </div>
                        </div>

                        <!-- Community Hall Card -->
                        <div class="property-item">
                            <a href="property-single.html" class="img">
                                <img src="~/Frontend/images/Bed1.jpg" alt="Community Hall" class="img-fluid" style="width: 100%; height: 500px;" />
                            </a>
                            <div class="property-content">
                                <div>
                                    <span class="city d-block mb-3">Community Hall</span>
                                    <span class="d-block mb-2 text-black-50">A spacious, fully air-conditioned hall with advanced audio-visual equipment, perfect for weddings, parties, and other celebrations. It includes seating arrangements for up to 300 guests.</span>
                                    <a href="property-single.html" class="btn btn-primary py-2 px-3">See details</a>
                                </div>
                            </div>
                        </div>

                        <!-- Seminar Hall Card -->
                        <div class="property-item">
                            <a href="property-single.html" class="img">
                                <img src="~/Frontend/images/Hall1.jpg" alt="Seminar Hall" class="img-fluid" style="width: 100%; height: 500px;" />
                            </a>
                            <div class="property-content">
                                <div>
                                    <span class="city d-block mb-3">Seminar Hall</span>
                                    <span class="d-block mb-2 text-black-50">Designed for corporate meetings and academic presentations, this hall is equipped with high-speed internet, projectors, and soundproof walls for a professional experience.</span>
                                    <a href="property-single.html" class="btn btn-primary py-2 px-3">See details</a>
                                </div>
                            </div>
                        </div>

                        <!-- Theater Card -->
                        <div class="property-item">
                            <a href="property-single.html" class="img">
                                <img src="~/Frontend/images/Theater.jpg" alt="Theater" class="img-fluid" style="width: 100%; height: 500px;" />
                            </a>
                            <div class="property-content">
                                <div>
                                    <span class="city d-block mb-3">Theater</span>
                                    <span class="d-block mb-2 text-black-50">An exclusive mini-theater for society members with high-definition screens, surround sound, and reclining seats, offering a premium movie-watching experience.</span>
                                    <a href="property-single.html" class="btn btn-primary py-2 px-3">See details</a>
                                </div>
                            </div>
                        </div>

                        <!-- Additional Hall Card -->
                        <div class="property-item">
                            <a href="property-single.html" class="img">
                                <img src="~/Frontend/images/Hall3.jpg" alt="Additional Hall" class="img-fluid" style="width: 100%; height: 500px;" />
                            </a>
                            <div class="property-content">
                                <div>
                                    <span class="city d-block mb-3">Additional Hall</span>
                                    <span class="d-block mb-2 text-black-50">This multi-purpose hall can accommodate up to 500 guests, making it ideal for conferences, cultural events, and larger community functions.</span>
                                    <a href="property-single.html" class="btn btn-primary py-2 px-3">See details</a>
                                </div>
                            </div>
                        </div>

                    </div>

                    <div id="property-nav" class="controls" tabindex="0" aria-label="Carousel Navigation">
                        <span class="prev" data-controls="prev" aria-controls="property" tabindex="-1">Prev</span>
                        <span class="next" data-controls="next" aria-controls="property" tabindex="-1">Next</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



